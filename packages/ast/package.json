{
  "name": "@rsql/ast",
  "description": "RSQL AST definitions and functions",
  "repository": "https://github.com/piotr-oles/rsql.git",
  "license": "MIT",
  "author": "Piotr Ole≈õ <piotrek.oles@gmail.com>",
  "version": "1.0.0",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "rsql",
    "fiql",
    "ast"
  ],
  "scripts": {
    "build:clean": "rm -rf ./lib && rm -f tsconfig.tsbuildinfo && rm -rf ./dist",
    "build:lib": "tsc -b tsconfig.json",
    "build:dist": "rollup -c && dts-bundle --name \"@rsql/ast\" --main lib/index.d.ts --out ../dist/index.d.ts --outputAsModuleFolder && prettier --write ./dist",
    "build": "yarn build:clean && yarn build:lib && yarn build:dist"
  },
  "engines": {
    "node": ">= 8"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
